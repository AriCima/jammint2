{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "indent":["error", 4],
        "react/jsx-filename-extension": 0,
        "react/jsx-indent" : ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "react/prop-types": 0,
        "arrow-parens": 0
    }
}